{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "extends": [
        "plugin:v3xlabs/recommended"
    ],
    "ignorePatterns": [
        "!**/*",
        "src/**/*.gen.ts"
    ],
    "plugins": [
        "v3xlabs"
    ],
    "env": {
        "browser": true
    },
    "rules": {
        "sonarjs/cognitive-complexity": "off"
    }
}
